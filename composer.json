{
    "name": "atl-software/contacts",
    "type": "project",
    "description": "ATL Software Testing Project",
    "license": "MIT",
    "require": {
        "php": "^8.0.0",
        "ext-pdo": "*",
        "ext-pdo_mysql": "*",
        "vlucas/phpdotenv": "^5.4"
    },
    "autoload": {
        "psr-4": {
            "AtlSoftware\\": "app/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
